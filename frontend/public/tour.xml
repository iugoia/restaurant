<action name="get_coordinates">
screentosphere(mouse.x, mouse.y, toh, tov); roundval(toh, 2); roundval(tov, 2); txtadd(txt, 'ath="', get(toh), '" atv="', get(tov), '"'); copy(plugin[clipboard].text, txt); trace('ath=', toh, ' atv=', tov); copy(hotspot[testspot].ath, toh); copy(hotspot[testspot].atv, tov); //set(hotspot[testspot].visible, true); //trace(px, ' ', py);
</action>

<events name="coordinates" onmousedown="get_coordinates();" keep="true"/>

	<!-- startup action - load the first scene -->

<action name="startup" autorun="onstart">
set(prev_name, null); if(startscene === null OR !scene[get(startscene)], copy(startscene, scene[0].name); ); loadscene(get(startscene), null, MERGE);   set(info_visible, false);  set(contur_visible, false); callwith(layer[icons_button], icons_button_click());  //create_watermarks(); 
</action>


<action name="new_scene">
set(layer[icons_button].visible, calc(scene[get(xml.scene)].index == 0 ? true : false)); 

</action>

